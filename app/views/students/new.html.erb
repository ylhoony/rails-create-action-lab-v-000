<h1>Student Form</h1>

<% form_tag students_path do %>

  <label for="first_name">first_name</label><br>
  <%= text_field_tag :first_name %><br>

  <label for="last_name">last_name</label><br>
  <%= text_field_tag :last_name %><br>

  <%= submit_tag "Submit" %>

<% end %>
